<p id="notice"><%= notice %></p>

<h1>Listing Download Urls</h1>

<table>
  <thead>
    <tr>
      <th>Token</th>
      <th>Url</th>
      <th>Title</th>
      <th>Notes</th>
      <th>Mimetype</th>
      <th>Creator</th>
      <th>Enabled</th>
      <th>Request ip</th>
      <th>Request user agent</th>
      <th>Accessed at</th>
      <th>Download completed at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @download_urls.each do |download_url| %>
      <tr>
        <td><%= download_url.token %></td>
        <td><%= download_url.url %></td>
        <td><%= download_url.title %></td>
        <td><%= download_url.notes %></td>
        <td><%= download_url.mimetype %></td>
        <td><%= download_url.creator %></td>
        <td><%= download_url.enabled %></td>
        <td><%= download_url.request_ip %></td>
        <td><%= download_url.request_user_agent %></td>
        <td><%= download_url.accessed_at %></td>
        <td><%= download_url.download_completed_at %></td>
        <td><%= link_to 'Show', download_url %></td>
        <td><%= link_to 'Edit', edit_download_url_path(download_url) %></td>
        <td><%= link_to 'Destroy', download_url, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Download url', new_download_url_path %>
